                  span.icon-bar
                a.navbar-brand(href='index.html'): img(src="images/logo.png")
              .navbar-collapse.collapse.overlay
                ul.nav.navbar-nav.navbar-right
                  li
                    a(href='index.html') Acasa
                  li
