      .container-fluid
        .header
          .header-text
            h1 Contact


      .row
        .contact-info
          .container.text-center
            h3.text-center Contact Info
            hr
            .col-md-6
              .fa.fa-phone.fa-2x  0751 333 999
              br
              .fa.fa-envelope.fa-2x   contact@rotaract.com
            .col-md-6
              .fa.fa-user.fa-2x   Radu Bores
              br
              .fa.fa-black-tie.fa-2x   Manager
      .row
        .contact-info
          .container
            form(method="post" action="contactengine.php")
              .col-md-6
                label(for="Name")
                input(type="text" class="feedback-input form-control" placeholder="Name" name="Name" id="Name")

                label(for="Tel")
                input(type="text" class="feedback-input form-control" placeholder="Phone Number" name="Tel" id="Tel")

                label(for="Email")
                input(type="text" class="feedback-input form-control" placeholder="E-mail" name="Email" id="Email")

              .col-md-6
                label(for="Message")
                textarea(type="text" class="feedback-input form-control" placeholder="Message..." rows="5" cols="20" id="Message")
                br
                input(type="submit" name="submit" value="Submit" class="submit-button form-control")
            
      .row
          .container-fluid
            #map-contact
              .overlay



      .row            
      .main-partners
        h1.text-center Parteneri
        .partners
          a(href="#"): img(src="images/download.png")
          a(href="#"): img(src="images/carlogo.png")
          a(href="#"): img(src="images/ooo-main-logo-2col-trans.png")


